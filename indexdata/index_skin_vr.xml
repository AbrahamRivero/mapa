<?xml version="1.0" encoding="UTF-8"?>
<krpano version="1.19-pr16">

    
  <!-- Spot Style - Enlace -->


  <style name="IconEnlace"
         url="%FIRSTXML%/spots/hotspot_image_sequence_user_defaultpoint1.png"
         visible="false"
         frame="0"
         lastframe="12"
         framewidth="50"
         frameheight="50"
         stopspotsequence="false"
         crop="0|0|50|50"
         edge="center"
         onloaded="startspotanimation(0.1);"
         onhover="onhoverIconEnlace"
         onout="onoutIconEnlace"


         />
  <action name="onhoverIconEnlace">
    showtooltip('hotspot');
  </action>
  <action name="onoutIconEnlace">
    hideTooltip();



  </action>



    
